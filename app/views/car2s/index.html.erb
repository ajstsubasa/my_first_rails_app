<p id="notice"><%= notice %></p>

<h1>Car2s</h1>

<table>
  <thead>
    <tr>
      <th>Make</th>
      <th>Model</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car2s.each do |car2| %>
      <tr>
        <td><%= car2.make %></td>
        <td><%= car2.model %></td>
        <td><%= car2.year %></td>
        <td><%= link_to 'Show', car2 %></td>
        <td><%= link_to 'Edit', edit_car2_path(car2) %></td>
        <td><%= link_to 'Destroy', car2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car2', new_car2_path %>
